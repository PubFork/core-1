@stroke shadowStroke { 1 #6 }
@stroke highlightStroke { 1 #DF }

@container test
{
    width: 200;
    height: 200;
    background: #A;
    stroke: shadowStroke, highlightStroke;
    shape: @{ 10 };
    alignX, alignY: middle;
}

*
{
    background: #C;

    #new test
    {
        isA: test;
    }
}
